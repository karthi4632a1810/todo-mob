{
  "expo": {
    "name": "Todo App",
    "slug": "todo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.mapims.todoapp",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to your camera to take profile photos.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to select profile photos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "org.mapims.todoapp",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you set your profile picture.",
          "cameraPermission": "The app accesses your camera to let you take a profile picture."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "ff509c36-4fc2-4d5e-8ef3-d2c58dcb9de8"
      }
    },
    "owner": "karthi18102000"
  }
}
